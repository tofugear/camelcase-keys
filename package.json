	"dependencies": {
		"camelcase": "^5.3.1",
		"map-obj": "^4.0.0",
		"quick-lru": "^4.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-external-helpers": "^7.8.3",
